<h1>List of blogs</h1>
<p><%= link_to "Create New Blog", new_post_path %></p>
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>

  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.description %></td>
      <td><%= link_to 'Edit', edit_post_path(post) %></td>
      <td><%= link_to 'Show', post_path(post) %></td>

    </tr>
  <% end %>

</table>
