<h1 align="center">Category Name:
  <%= @category.name %>
</h1>

<div align="center">
  <% if logged_in? && current_user.admin? %>
  <div class="row">
    <span class="badge"><%= link_to "Edit Name of Category", edit_category_path(@category) %></span>
  </div>
  <% end %>
    <%= will_paginate @category_posts %>
</div>
<%= render 'posts/post', obj: @category_posts %>
  <div align="center">
      <%= will_paginate @category_posts %>
  </div>
